{
  "manifest_version": 2,

  "name": "wipe",
  "description": "wipe local-/sessionstorage and cookies",
  "version": "1.1",

  "browser_action": {
   "default_icon": "axe.png",
   "default_title": "wipe!"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": ["wipe.js"],
	"persistent": false
  }
}
